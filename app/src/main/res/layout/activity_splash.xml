<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorWhite"
    tools:context=".startApp.SplashActivity">

    <ImageView
        android:id="@+id/launch_R"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="20dp"
        android:src="@drawable/launch_r_w"
        app:layout_constraintBottom_toBottomOf="@+id/launch_S"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/launch_S"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:src="@drawable/launch_s_w"
        app:layout_constraintEnd_toStartOf="@+id/launch_S2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/launch_R"
        app:layout_constraintTop_toTopOf="@+id/launch_S2" />

    <ImageView
        android:id="@+id/launch_S2"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="240dp"
        android:layout_marginEnd="20dp"
        android:src="@drawable/launch_s2_w"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/launch_read"
        android:layout_width="118dp"
        android:layout_height="33dp"
        android:layout_marginStart="24dp"
        android:layout_marginBottom="4dp"
        android:src="@drawable/launch_read"
        app:layout_constraintBottom_toTopOf="@+id/launch_R"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/launch_simple"
        android:layout_width="157dp"
        android:layout_height="33dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:src="@drawable/launch_simple"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/launch_S" />

    <ImageView
        android:id="@+id/launch_slider"
        android:layout_width="133dp"
        android:layout_height="33dp"
        android:layout_marginBottom="4dp"
        android:src="@drawable/launch_slider"
        app:layout_constraintBottom_toTopOf="@+id/launch_S2"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/launch_by"
        android:layout_width="388dp"
        android:layout_height="207dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:src="@drawable/launch_by"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.733"
        app:layout_constraintStart_toStartOf="parent" />



</androidx.constraintlayout.widget.ConstraintLayout>